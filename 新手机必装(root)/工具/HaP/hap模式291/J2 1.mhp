<script type="text/javascript">
	function isclick(){
	    var box = document.getElementById("ischecked").checked; 	 
		if(box){
		        var Days = 20;  //此 cookie 将被保存 20 天
		        var exp = new Date();  
		        exp.setTime(exp.getTime() + Days*24*60*60*1000);
	            document.cookie ="redirect_advice_cookie=0;expires=" + exp.toGMTString();
	            return;
		}else{  
				return;
		     }
	}	
	function isclick2(){
	    var box = document.getElementById("ischecked"); 
		if(!box.checked){
		box.checked = true;
		}else{
		box.checked = false;
		}
	}
	
	window.onload = getWidth;
    function getWidth() {
    	try {
    		var width = document.body.clientWidth;
    		var ad_w = 320;
    		var img1 = document.getElementById("img1");
    		var ads = document.getElementById("ads");
    		if(width > 480){
    			if(null != img1) {
    				img1.style.width = ad_w+"px";
    			}
    			if(null != ads) {
    				ads.style.height = (ad_w/320)*60;
    			}
    		}
    		else {
    			if(null != img1) {
    				img1.style.width = width+"px";
    			}
    			if(null != ads) {
    				ads.style.height = width*0.1875+"px";
    			}
    		}
    	} catch(ex) {
    	}
	}
    </script>
